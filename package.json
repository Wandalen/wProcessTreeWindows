{
  "name": "wwindows-process-tree",
  "version": "0.2.4",
  "description": "Fetch a Windows process tree fast",
  "main": "lib/index.js",
  "types": "typings/windows-process-tree.d.ts",
  "scripts": {
    "test": "mocha lib/test.js",
    "install": "node-pre-gyp install --update-binary --fallback-to-build",
    "node-pre-gyp-install": "node-pre-gyp install --update-binary --fallback-to-build",
    "node-pre-gyp-build": "node-pre-gyp configure && node-pre-gyp rebuild",
    "node-pre-gyp-package": "node-pre-gyp package",
    "node-pre-gyp-github-release": "node-pre-gyp-github publish --release"
  },
  "files": [ "binding.gyp", "typings", "src", "lib" ],
  "binary": {
    "module_name": "windows_process_tree",
    "module_path": "./binding/",
    "host": "https://github.com/volodymyrmigdal/vscode-windows-process-tree/releases/download/",
    "remote_path": "{version}"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/volodymyrmigdal/vscode-windows-process-tree.git"
  },
  "homepage": "https://github.com/volodymyrmigdal/vscode-windows-process-tree",
  "bugs": {
    "url": "https://github.com/volodymyrmigdal/vscode-windows-process-tree/issues"
  },
  "dependencies": {
    "nan": "^2.13.2"
  },
  "gypfile" : false,
  "devDependencies": {
    "mocha": "^3.5.0",
    "typescript": "2.7.2",
    "tslint": "5.9.1",
    "@types/node": "9.4.7",
    "@types/mocha": "2.2.48",
    "node-gyp": "5.1.1",
    "node-pre-gyp": "0.14.0",
    "node-pre-gyp-github": "1.4.3"
  },
  "os": [ "win32" ]
}
