    "@mapbox/node-pre-gyp": "1.0.5"
  },
  "gypfile": false,
  "devDependencies": {
    "typescript": "2.7.2",
    "tslint": "5.9.1",
    "mocha": "^3.5.0",
    "eslint": "7.1.0",
    "@types/node": "9.4.7",
